    <sql>SELECT 

CONCAT(
	CONCAT(
		CONCAT (&apos;SELECT * FROM &apos;, TABLE_NAME, &apos; WHERE HOSTED_CLIENT_SK=&apos;), ${hosted_client_sk}, &apos; INTO OUTFILE &quot;&apos;
	),
CONCAT(?, TABLE_NAME,&apos;.txt&quot;&apos;)
,&apos;;&apos;) AS FIELD1
FROM ADMIN_MODULE_TABLE T, ADMIN_MODULE_TABLE_TABLE TT, ADMIN_MODULE M
WHERE T.TABLE_SK = TT.TABLE_SK 
AND TT.MODULE_SK = M.MODULE_SK 
AND M.MODULE_NAME = ?</sql>
