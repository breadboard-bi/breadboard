    <sql>SELECT IT.INTERACTION_ID, 
0 AS SEQUENCE_NUMBER,
IT.BUSINESS_UNIT AS BUSINESS_UNIT_ID,
IT.ROOT_INTER_ID AS ROOT_INTERACTION_ID,  
IT.PARENT_INTER_ID AS PARENT_INTERATION_ID, 
IT.INTERACT_BEGIN AS INTERACTION_BEGIN_DATE, 
IT.INTERACT_END AS INTERACTION_END_DATE
FROM PS_RI_INTERACTION IT
WHERE NOT EXISTS (select &apos;1&apos; from PS_RI_SUB_INTERACT SI WHERE SI.INTERACTION_ID = IT.INTERACTION_ID)
--and IT.LASTUPP_EW_DTTM &gt; last load date	
UNION ALL
SELECT SI.INTERACTION_ID, 
SI.SEQNUM AS SEQUENCE_NUMBER, 
SI.BUSINESS_UNIT_RI AS BUSINESS_UNIT_ID,
IT.ROOT_INTER_ID AS ROOT_INTERACTION_ID, 
IT.PARENT_INTER_ID AS PARENT_INTERACTION_ID, 
SI.INTERACT_BEGIN AS INTERACTION_BEGIN_DATE, 
SI.INTERACT_END AS INTERACTION_END_DATE
FROM PS_RI_SUB_INTERACT SI, PS_RI_INTERACTION IT
WHERE IT.INTERACTION_ID = SI.INTERACTION_ID
--and SI.LASTUPP_EW_DTTM &gt; last load date</sql>
