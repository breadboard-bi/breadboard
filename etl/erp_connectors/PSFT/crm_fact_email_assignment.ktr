  <step>
    <name>PSFT_CRM_EMAIL_ASSIGNMENT_METRICS</name>
    <type>TableInput</type>
    <description></description>
    <distribute>Y</distribute>
    <copies>1</copies>
    <connection>pf890_sqlserver_pc-xp</connection>
    <sql>SELECT 
EH.ROW_ADDED_DTTM AS ASSIGNMENT_DATE, 
EM.BUSINESS_UNIT AS BUSINESS_UNIT_ID,
EH.MCF_EMAIL_ID AS EMAIL_ID,
EH.SEQ_NUM AS ASSIGNMENT_SEQUENCE_NUMBER, 
EH.MAILBOX_ID AS MAILBOX_ID, 
EH.CURRENT_WL AS WORKGROUP_NAME, 
EH.ASSIGNED_TO AS AGENT_ASSIGNED_TO_ID,  --NEED OPERATOR TABLE FOR LOOKUP? 
EH.EMAIL_STATUS AS INTERACTION_STATUS_ID,
EM.ROW_ADDED_DTTM, 
EM.CLOSED_DTTM,
(CASE WHEN EH.SEQ_NUM = 1 THEN EM.ROW_ADDED_DTTM - EH.ROW_ADDED_DTTM
ELSE (SELECT EH2.ROW_ADDED_DTTM FROM PS_RB_INEM_HIST EH2 WHERE EH2.MCF_EMAIL_ID = EH.MCF_EMAIL_ID and EH2.SEQ_NUM = EH.SEQ_NUM - 1) - EH.ROW_ADDED_DTTM --Previous assignment date.  Verify with real data.
END) AS ASSIGNMENT_DURATION
FROM 
PS_RB_INEM_HIST EH, 
PS_RB_IN_EMAIL EM
WHERE EH.MCF_EMAIL_ID = EM.MCF_EMAIL_ID 
and EH.EMAIL_STATUS in (&apos;ASGN&apos;,&apos;RASN&apos;)
-- and EH.LASTUPD_EW_DTTM &gt; last load date</sql>
