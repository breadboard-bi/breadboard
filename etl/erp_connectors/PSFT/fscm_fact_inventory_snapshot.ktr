  <step>
    <name>PSFT_FSCM_INVENTORY_SNAPSHOT</name>
    <type>TableInput</type>
    <description>This step represents a subset of the PS_RA_CAMPAIGN table.  It maintains all marketing campaign program information, such as name, start and end dates, relationship to a parent (roll up), overall campaign status, objective and description.</description>
    <distribute>Y</distribute>
    <copies>1</copies>
    <connection>pf890_sqlserver_pc-xp</connection>
    <sql>SELECT CAST (CONVERT(CHAR(10),GETDATE(),112) as NUMERIC) AS SNAPSHOT_DATE_SK, --DBMS SPECIFIC
GETDATE() AS SNAPSHOT_DATE,
RTRIM(INV.INV_LOT_ID) AS INV_LOT_ID,
RTRIM(INV.INV_ITEM_ID) AS INV_ITEM_ID,
RTRIM(INV.BUSINESS_UNIT) AS BUSINESS_UNIT_ID,
RTRIM(MASTER_ITEM_TBL.SETID) AS SETID,
CASE WHEN INV.AVAIL_STATUS = &apos;1&apos; THEN 1 ELSE 0 END AS INVENTORY_OPEN_QTY,
CASE WHEN INV.AVAIL_STATUS = &apos;2&apos; THEN 1 ELSE 0 END AS INVENTORY_RESTRICTED_QTY, 
CASE WHEN INV.AVAIL_STATUS = &apos;3&apos; THEN 1 ELSE 0 END AS INVENTORY_HOLD_QTY, 
CASE WHEN INV.AVAIL_STATUS = &apos;4&apos; THEN 1 ELSE 0 END AS INVENTORY_REJECTED_QTY 
FROM PS_PHYSICAL_INV INV
LEFT OUTER JOIN PS_SET_CNTRL_REC MASTER_ITEM_TBL
ON INV.BUSINESS_UNIT = MASTER_ITEM_TBL.SETCNTRLVALUE
AND MASTER_ITEM_TBL.RECNAME = &apos;MASTER_ITEM_TBL&apos;</sql>
