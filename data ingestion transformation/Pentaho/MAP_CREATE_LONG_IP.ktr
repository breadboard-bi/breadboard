  <step>
    <name>CREATE_LONG_IP</name>
    <type>ScriptValue</type>
    <description>This Java Script Value step creates the long IP address for subsequent use in determining the organization and geographic location of the visitor.</description>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <compatible>Y</compatible>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>ScriptValue</jsScript_name>
        <jsScript_script>&#47;&#47; Converts an IP to a Long IP address

var ip_pieces = IP_ADDRESS_DESC.getString().split(&quot;\\.&quot;);

var v_part1 = parseFloat(ip_pieces[0]) * 16777216;
var v_part2 = parseFloat(ip_pieces[1]) * 65536;
var v_part3 = parseFloat(ip_pieces[2]) * 256;
var v_part4 = parseFloat(ip_pieces[3]);

var LONG_IP_NUMBER = v_part1 + v_part2 + v_part3 + v_part4;
</jsScript_script>
      </jsScript>    </jsScripts>    <fields>      <field>        <name>LONG_IP_NUMBER</name>
        <rename>LONG_IP_NUMBER</rename>
