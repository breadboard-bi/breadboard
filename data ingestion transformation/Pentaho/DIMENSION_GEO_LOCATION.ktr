  <step>
    <name>STAGE_GEO_CITY_LOCATION</name>
    <type>TableInput</type>
    <description>This step extracts data from the Breadboard BI stage table.</description>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
