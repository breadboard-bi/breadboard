    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>ScriptValue</jsScript_name>
        <jsScript_script>&#47;&#47; Declare Variables
var v_curDate = REQUEST_DATE_SK.getNumber();
var v_cur_WEB_VISITOR_SK = WEB_VISITOR_SK.getNumber();
var v_cur_SOURCE_SYSTEM_SK = SOURCE_SYSTEM_SK.getNumber();
var v_cur_HOSTED_CLIENT_SK = HOSTED_CLIENT_SK.getNumber();
var v_cur_WEB_FILE_SK = WEB_FILE_SK.getNumber();
var v_web_file_name = WEB_FILE_NAME.getString();
var v_view_minutes_qty = (VIEW_SECONDS_QTY.getNumber())&#47;60;
var v_WEB_NAVIGATION_PATH_DESC = &quot;Unknown&quot;;

&#47;&#47; Check Subsequent Views - same file is NOT repeated in path
if (v_prev_WEB_FILE_SK == v_cur_WEB_FILE_SK 
&amp;&amp; v_cur_HOSTED_CLIENT_SK == v_prev_HOSTED_CLIENT_SK 
&amp;&amp; v_cur_SOURCE_SYSTEM_SK == v_prev_SOURCE_SYSTEM_SK 
&amp;&amp; v_prevDate == v_curDate 
&amp;&amp; v_cur_WEB_VISITOR_SK == v_prev_WEB_VISITOR_SK 
&amp;&amp; v_view_minutes_qty &lt; 31)
{
v_WEB_NAVIGATION_PATH_DESC = v_prev_web_navigation_path_desc;
}

&#47;&#47; Check Subsequent Views different file
else if (v_cur_HOSTED_CLIENT_SK == v_prev_HOSTED_CLIENT_SK 
&amp;&amp; v_cur_SOURCE_SYSTEM_SK == v_prev_SOURCE_SYSTEM_SK 
&amp;&amp; v_prevDate == v_curDate 
&amp;&amp; v_cur_WEB_VISITOR_SK == v_prev_WEB_VISITOR_SK 
&amp;&amp; v_view_minutes_qty &lt; 31)
{
v_WEB_NAVIGATION_PATH_DESC = v_prev_web_navigation_path_desc + &quot; &gt; &quot; + v_web_file_name;
}

else {
v_WEB_NAVIGATION_PATH_DESC = v_web_file_name;

}

&#47;&#47; Set current values as previous for the next row
var v_prev_SOURCE_SYSTEM_SK =&quot;&quot;;
var v_prev_HOSTED_CLIENT_SK =&quot;&quot;;
var v_prev_WEB_VISITOR_SK =&quot;&quot;;
var v_prevDate = &quot;&quot;;
var v_prev_web_navigation_path_desc = &quot;&quot;;
var v_prev_web_file_name = &quot;&quot;;
var v_prev_WEB_FILE_SK = &quot;&quot;;
v_prev_WEB_VISITOR_SK = v_cur_WEB_VISITOR_SK;
v_prev_SOURCE_SYSTEM_SK = v_cur_SOURCE_SYSTEM_SK;
v_prev_HOSTED_CLIENT_SK = v_cur_HOSTED_CLIENT_SK;
v_prev_WEB_FILE_SK = v_cur_WEB_FILE_SK;
v_prevDate = v_curDate;
v_prev_web_navigation_path_desc = v_WEB_NAVIGATION_PATH_DESC;
v_prev_web_file_name = v_web_file_name;</jsScript_script>
