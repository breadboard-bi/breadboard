    <connection>pf890_sqlserver_pc-xp</connection>
    <sql>SELECT S1.EEEMAN8 AS SALES_PERSON_ID,
S2.ABALPH AS SALES_PERSON_NAME,
S1.EEENUM AS SALES_PERSON_EMPLOYEE_NUMBER,
S1.EEACTIV AS ACTIVE_IND,
S1.EEHDATE AS HIRE_DATE,
S1.EEDPTCD AS SALES_PERSON_DEPT_ID,
S3.DRDL01 AS SALES_PERSON_DEPT_DESC
FROM PS_S_F90CA040 S1
LEFT OUTER JOIN PS_S_F0101 S2
ON S1.EEEMAN8 = S2.ABAN8 --VERIFY
LEFT OUTER JOIN PS_S_F0005 S3
ON S1.EEDPTCD = S3.DRKY
AND LTRIM(RTRIM(S3.DRSY)) = &apos;90CA&apos;
AND LTRIM(RTRIM(S3.DRRT)) = &apos;DC&apos;</sql>
