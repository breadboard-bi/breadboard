    <connection>compiere</connection>
    <sql>SELECT A.AD_USER_ID
, A.NAME
, A.C_BPARTNER_ID
, A.AD_ORG_ID
FROM COMPIERE.C_BPARTNER P, COMPIERE.AD_USER A
WHERE P.C_BPARTNER_ID = A.C_BPARTNER_ID
AND (P.ISCUSTOMER = &apos;Y&apos; OR P.ISPROSPECT = &apos;Y&apos;)</sql>
