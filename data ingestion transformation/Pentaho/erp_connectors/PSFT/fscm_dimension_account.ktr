  <step>
    <name>PSFT_FSCM_ACCOUNT</name>
    <type>TableInput</type>
    <description>This step represents a subset of the columns from the PS_RSF_SUSER table.  It maintains the basic sales user record with descriptors and settings.
</description>
    <distribute>Y</distribute>
    <copies>1</copies>
    <connection>pf890_sqlserver_pc-xp</connection>
    <sql>SELECT RTRIM(GL_ACCOUNT.SETID) AS SETID,
RTRIM(GL_ACCOUNT.ACCOUNT) AS ACCOUNT_ID,
GL_ACCOUNT.EFFDT AS EFFECTIVE_DATE,
RTRIM(GL_ACCOUNT.DESCR) AS ACCOUNT_DESC,
RTRIM(GL_ACCOUNT.DESCRSHORT) AS ACCOUNT_NAME,
RTRIM(GL_ACCOUNT.ACCOUNT_TYPE) AS ACCOUNT_TYPE_ID,
RTRIM(ACCOUNT.DESCR) AS ACCOUNT_TYPE_DESC
FROM PS_GL_ACCOUNT_TBL GL_ACCOUNT
LEFT OUTER JOIN PS_ACCT_TYPE_TBL ACCOUNT
ON GL_ACCOUNT.ACCOUNT_TYPE = ACCOUNT.ACCOUNT_TYPE 
AND GL_ACCOUNT.SETID = ACCOUNT.SETID
WHERE GL_ACCOUNT.EFFDT = 
(SELECT MAX(EFFDT) 
FROM PS_GL_ACCOUNT_TBL GL_ACCOUNT2 
WHERE GL_ACCOUNT.SETID = GL_ACCOUNT2.SETID 
AND GL_ACCOUNT.ACCOUNT=GL_ACCOUNT2.ACCOUNT)</sql>
