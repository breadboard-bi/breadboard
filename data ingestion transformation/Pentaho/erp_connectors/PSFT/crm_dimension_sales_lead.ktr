    <type>TableInput</type>
    <description></description>
    <distribute>Y</distribute>
    <copies>1</copies>
    <connection>pf890_sqlserver_pc-xp</connection>
    <sql>select 
LEAD.LEAD_ID,
LEAD.BUSINESS_UNIT AS BUSINESS_UNIT_ID,
LEAD.PERSON_ID AS LEAD_OWNER_PERSON_ID, --TBC
LEAD.SALES_USER_NAME AS LEAD_OWNER_NAME, --TBC
LEAD.LEAD_TYPE AS LEAD_TYPE_ID,
LEAD_TYPE.XLATSHORTNAME AS LEAD_TYPE_NAME,
LEAD_TYPE.XLATLONGNAME AS LEAD_TYPE_DESC,
LEAD.DESCR50 AS LEAD_DESC
from PS_RSF_LEAD LEAD
LEFT OUTER JOIN PSXLATITEM LEAD_TYPE
ON LEAD_TYPE.FIELDNAME = &apos;LEAD_TYPE&apos;
AND LEAD.LEAD_TYPE = LEAD_TYPE.FIELDVALUE
AND LEAD_TYPE.EFFDT = (SELECT MAX(A1.EFFDT) 
	FROM PSXLATITEM A1
	WHERE LEAD_TYPE.FIELDNAME = A1.FIELDNAME 
	AND LEAD_TYPE.FIELDVALUE = A1.FIELDVALUE
	AND LEAD_TYPE.FIELDNAME = &apos;LEAD_TYPE&apos;)</sql>
    <limit>0</limit>
    <lookup></lookup>
    <execute_each_row>N</execute_each_row>
