  <step>
    <name>PSFT_INVENTORY_ITEM</name>
    <type>TableInput</type>
    <description>This step represents a subset of the columns from the PS_RSF_SUSER table.  It maintains the basic sales user record with descriptors and settings.
</description>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>pf890_sqlserver_pc-xp</connection>
    <sql>SELECT RTRIM(V.VENDOR_ID) AS SUPPLIER_ID,
RTRIM(V.SETID) AS SUPPLIER_SETID,
RTRIM(V.VENDOR_NAME_SHORT) AS SUPPLIER_NAME ,
RTRIM(V.NAME1) AS SUPPLIER_DESC,
RTRIM(V.VENDOR_CLASS) AS SUPPLIER_CLASS_CODE,
RTRIM(V.VENDOR_STATUS) AS SUPPLIER_STATUS_CODE,
RTRIM(A.ADDRESS1) AS ADDRESS_LINE1_DESC,
RTRIM(A.ADDRESS2) AS ADDRESS_LINE2_DESC,
RTRIM(A.ADDRESS3) AS ADDRESS_LINE3_DESC,
RTRIM(A.ADDRESS4) AS ADDRESS_LINE4_DESC,
RTRIM(A.CITY) AS CITY_NAME,
RTRIM(A.POSTAL) AS POSTAL_CODE,
RTRIM(N.STD_ID_NUM) AS DUNS_NUMBER
FROM PS_VENDOR V
LEFT OUTER JOIN PS_VENDOR_ADDR A
ON  V.VENDOR_ID = A.VENDOR_ID 
AND  V.SETID = A.SETID 
AND  V.PRIM_ADDR_SEQ_NUM = A.ADDRESS_SEQ_NUM 
AND  A.EFF_STATUS = &apos;A&apos; 
&#47;* Restricts to latest active address, remove if not desired *&#47;
AND A.EFFDT= 
	(SELECT MAX(EFFDT) FROM PS_VENDOR_ADDR 
	WHERE VENDOR_ID = PS_VENDOR_ADDR.VENDOR_ID 
	AND SETID = PS_VENDOR_ADDR.SETID 
	AND ADDRESS_SEQ_NUM = PS_VENDOR_ADDR.ADDRESS_SEQ_NUM)
LEFT OUTER JOIN PS_VENDOR_ID_NBRS N
ON V.VENDOR_ID = N.VENDOR_ID 
AND V.SETID = N.STD_ID_SETID
AND N.STD_ID_NUM_QUAL = &apos;DB&apos; </sql>
