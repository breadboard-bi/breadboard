  <step>
    <name>PSFT_CRM_CASE_SURVEY_METRICS</name>
    <type>TableInput</type>
    <description></description>
    <distribute>Y</distribute>
    <copies>1</copies>
    <connection>pf890_sqlserver_pc-xp</connection>
    <sql>SELECT 
BI.ROW_ADDED_DTTM AS SURVEY_DATE,
BI.RC_BS_INSTANCE AS SURVEY_INSTANCE_ID, 
CS.BUSINESS_UNIT AS BUSINESS_UNIT_ID, 
CS.CASE_ID,
CS.BO_ID_CUST AS BO_CUSTOMER_ID, 
CS.PRODUCT_ID, 
CS.INST_PROD_ID AS INSTALLED_PRODUCT_ID, 
CS.ASSIGNED_TO AS AGENT_ASSIGNED_ID, 
CS.PROVIDER_GRP_ID AS PROVIDER_GROUP_ID,
BI.SCRIPT_ID, 
SR.RC_RATE_ID AS RATE_ID, 
BI.SETID AS SURVEY_INSTANCE_SETID,
SR.RC_WEIGHT AS SCRIPT_WEIGHT,
ANSWER.ANSWER_SCORE/SR.RC_WEIGHT AS SURVEY_SCORE,
ANSWER.ANSWER_SCORE
--RATE_SCORE NOT INCLUDED
FROM PS_RC_BS_INSTANCE BI
INNER JOIN PS_RC_CASE CS
ON BI.RC_BS_INSTANCE = CS.RC_BS_INSTANCE 
INNER JOIN PS_RC_BSCRIPT SR
ON BI.SETID = SR.SETID
AND BI.EFFDT = SR.EFFDT
AND BI.SCRIPT_ID = SR.SCRIPT_ID
LEFT OUTER JOIN (SELECT RC_BS_INSTANCE, SUM(RC_SCORE * RC_WEIGHT) AS ANSWER_SCORE FROM PS_RC_BS_ANSWER GROUP BY RC_BS_INSTANCE) ANSWER
ON ANSWER.RC_BS_INSTANCE = BI.RC_BS_INSTANCE
--WHERE BI.LASTUPD_EW_DTTM &gt; Last Load Date</sql>
