    <type>TableInput</type>
    <description></description>
    <distribute>Y</distribute>
    <copies>1</copies>
    <connection>pf890_sqlserver_pc-xp</connection>
    <sql>SELECT PERIOD.TIMEFRAME_PERIOD AS TIMEFRAME_PERIOD_ID, 
PERIOD.DESCR AS TIMEFRAME_PERIOD_DESC, 
PERIOD.BEGIN_DT AS TIMEFRAME_BEGIN_DATE, 
PERIOD.END_DT AS TIMEFRAME_END_DATE, 
PERIOD.PERIOD_TYPE AS TIMEFRAME_PERIOD_TYPE,
PERIOD_TYPE.XLATLONGNAME AS TIMEFRAME_PERIOD_DESC
FROM PS_RSF_TF_PERIOD PERIOD

LEFT OUTER JOIN PSXLATITEM PERIOD_TYPE
ON PERIOD_TYPE.FIELDNAME = &apos;PERIOD_TYPE&apos;
AND PERIOD.PERIOD_TYPE = PERIOD_TYPE.FIELDVALUE
AND PERIOD_TYPE.EFFDT = (SELECT MAX(A1.EFFDT) 
	FROM PSXLATITEM A1
	WHERE PERIOD_TYPE.FIELDNAME = A1.FIELDNAME 
	AND PERIOD_TYPE.FIELDVALUE = A1.FIELDVALUE
	AND PERIOD_TYPE.FIELDNAME = &apos;PERIOD_TYPE&apos;)</sql>
    <limit>0</limit>
    <lookup></lookup>
