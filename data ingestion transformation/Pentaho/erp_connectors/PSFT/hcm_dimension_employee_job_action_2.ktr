  <step>
    <name>PSFT_HCM_EMPLOYEE_JOB_ACTION</name>
    <type>TableInput</type>
    <description>This step represents a subset of the columns from the PS_RO_HEADER (order header) table. It maintains the order header information.</description>
    <distribute>Y</distribute>
    <copies>1</copies>
    <connection>pf890_sqlserver_pc-xp</connection>
    <sql>SELECT RTRIM(JOB.EMPLID) AS EMPLOYEE_ID, --AK
RTRIM(JOB.EMPL_RCD) AS EMPLOYEE_RECORD_ID, --AK
JOB.EFFDT AS EFFECTIVE_DATE, --AK
CAST (CONVERT(CHAR(10),JOB.EFFDT,112) as NUMERIC) AS EFFECTIVE_DATE_SK, --DBMS SPECIFIC
JOB.EFFSEQ AS EFFECTIVE_SEQUENCE_NUMBER, --AK
JOB.ACTION_DT AS JOB_ACTION_DATE,
CAST (CONVERT(CHAR(10),JOB.ACTION_DT,112) as NUMERIC) AS JOB_ACTION_DATE_SK, --DBMS SPECIFIC
RTRIM(JOB.ACTION) AS JOB_ACTION_ID,
RTRIM(JOB.ACTION_REASON) AS JOB_ACTION_REASON_ID,
RTRIM(JOB.BUSINESS_UNIT) AS BUSINESS_UNIT_ID,
RTRIM(JOB_REC.SETID) AS SETID,
RTRIM(JOB.COMPANY) AS COMPANY_ID,
RTRIM(JOB.DEPTID) AS DEPARTMENT_ID,
RTRIM(JOB.JOBCODE) AS JOBCODE_ID,
RTRIM(JOB.LOCATION) AS LOCATION_ID,
RTRIM(JOB.POSITION_NBR) AS POSITION_ID,
RTRIM(JOB.SUPERVISOR_ID) AS SUPERVISOR_EMPLOYEE_ID,
RTRIM(JOB.PAYGROUP) AS PAYGROUP_ID,
RTRIM(JOB.SAL_ADMIN_PLAN) AS SALARY_ADMIN_PLAN_ID,
RTRIM(JOB.STEP) AS SALARY_STEP_ID,
RTRIM(JOB.GRADE) AS SALARY_GRADE_ID,
RTRIM(ACCOUNT_GL.BUSINESS_UNIT_GL) AS GL_BUSINESS_UNIT_ID,
RTRIM(ACCOUNT.ACCOUNT) AS ACCOUNT_ID,
RTRIM(ACCOUNT.FUND_CODE) AS FUND_ID,
RTRIM(ACCOUNT.CLASS_FLD) AS CLASS_ID,
RTRIM(ACCOUNT.OPERATING_UNIT) AS OPERATING_UNIT_ID,
RTRIM(ACCOUNT.CHARTFIELD1) AS CHARTFIELD1_ID,
RTRIM(ACCOUNT.CHARTFIELD2) AS CHARTFIELD2_ID,
RTRIM(ACCOUNT.CHARTFIELD3) AS CHARTFIELD3_ID,
RTRIM(ACCOUNT.PRODUCT) AS PRODUCT_ID,
RTRIM(JOB.CURRENCY_CD) AS CURRENCY_ID,
RTRIM(JOB.CONTRACT_NUM) AS CONTRACT_ID, --DIMENSION NEEDS TO BE BUILT AND CONNECTED.
RTRIM(JOB.ACCT_CD) AS ACCOUNT_CODE,
JOB.DEPT_ENTRY_DT AS DEPARTMENT_START_DATE,
CAST (CONVERT(CHAR(10),JOB.DEPT_ENTRY_DT,112) as NUMERIC) AS DEPARTMENT_START_DATE_SK, --DBMS SPECIFIC
JOB.GRADE_ENTRY_DT AS GRADE_START_DATE,
CAST (CONVERT(CHAR(10),GRADE_ENTRY_DT,112) as NUMERIC) AS GRADE_START_DATE_SK, --DBMS SPECIFIC
JOB.JOB_ENTRY_DT AS JOB_START_DATE,
CAST (CONVERT(CHAR(10),JOB.JOB_ENTRY_DT,112) as NUMERIC) AS JOB_START_DATE_SK, --DBMS SPECIFIC
JOB.POSITION_ENTRY_DT AS POSITION_START_DATE,
CAST (CONVERT(CHAR(10),POSITION_ENTRY_DT,112) as NUMERIC) AS POSITION_START_DATE_SK, --DBMS SPECIFIC
JOB.STEP_ENTRY_DT AS STEP_START_DATE,
CAST (CONVERT(CHAR(10),JOB.STEP_ENTRY_DT,112) as NUMERIC) AS STEP_START_DATE_SK, --DBMS SPECIFIC
JOB.HIRE_DT AS HIRE_DATE,
CAST (CONVERT(CHAR(10),JOB.HIRE_DT,112) as NUMERIC) AS HIRE_DATE_SK, --DBMS SPECIFIC
JOB.ASGN_START_DT AS ASSIGNMENT_START_DATE,
CAST (CONVERT(CHAR(10),JOB.ASGN_START_DT,112) as NUMERIC) AS ASSIGNMENT_START_DATE_SK, --DBMS SPECIFIC
JOB.ASGN_END_DT AS ASSIGNMENT_END_DATE,
CAST (CONVERT(CHAR(10),JOB.ASGN_END_DT,112) as NUMERIC) AS ASSIGNMENT_END_DATE_SK, --DBMS SPECIFIC
/* For report performance, some of these descriptions may be snowflaked. */
RTRIM(JOB.STD_HRS_FREQUENCY) AS STANDARD_HOURS_FREQUENCY_CODE,
RTRIM(STD_HRS_FREQUENCY.XLATSHORTNAME) AS STANDARD_HOURS_FREQUENCY_NAME,
RTRIM(STD_HRS_FREQUENCY.XLATLONGNAME) STANDARD_HOURS_FREQUENCY_DESC,
RTRIM(JOB.COMP_FREQUENCY) AS COMPENSATION_FREQUENCY_CODE,
RTRIM(COMP_FREQUENCY.XLATSHORTNAME) AS COMPENSATION_FREQUENCY_NAME,
RTRIM(COMP_FREQUENCY.XLATLONGNAME) AS COMPENSATION_FREQUENCY_DESC,
RTRIM(JOB.LABOR_AGREEMENT) AS LABOR_AGREEMENT_CODE,
RTRIM(LABOR_AGREEMENT.DESCRSHORT) AS LABOR_AGREEMENT_NAME,
RTRIM(LABOR_AGREEMENT.DESCR) AS LABOR_AGREEMENT_DESC,
RTRIM(JOB.EMPL_CLASS) AS EMPLOYEE_CLASS_CODE,
RTRIM(EMPL_CLASS.XLATSHORTNAME) AS EMPLOYEE_CLASS_NAME,
RTRIM(EMPL_CLASS.XLATLONGNAME) AS EMPLOYEE_CLASS_DESC,
RTRIM(JOB.BEN_STATUS) AS BENEFIT_STATUS_CODE,
RTRIM(BEN_STATUS.XLATSHORTNAME) AS BENEFIT_STATUS_NAME,
RTRIM(BEN_STATUS.XLATLONGNAME) AS BENEFIT_STATUS_DESC,
RTRIM(JOB.BENEFIT_SYSTEM) AS BENEFIT_SYSTEM_CODE,
RTRIM(BENEFIT_SYSTEM.XLATSHORTNAME) AS BENEFIT_SYSTEM_NAME,
RTRIM(BENEFIT_SYSTEM.XLATLONGNAME) AS BENEFIT_SYSTEM_DESC,
RTRIM(JOB.CLASS_INDC) AS CLASSIFICATION_CODE,
RTRIM(CLASS_INDC.XLATSHORTNAME) AS CLASSIFICATION_NAME,
RTRIM(CLASS_INDC.XLATLONGNAME) AS CLASSIFICATION_DESC,
RTRIM(JOB.EARNS_DIST_TYPE) AS EARNINGS_DISTRIBUTION_CODE,
RTRIM(EARNS_DIST_TYPE.XLATSHORTNAME) AS EARNINGS_DISTRIBUTION_NAME,
RTRIM(EARNS_DIST_TYPE.XLATLONGNAME) AS EARNINGS_DISTRIBUTION_DESC,
RTRIM(JOB.EMPL_STATUS) AS EMPLOYEE_STATUS_CODE,
RTRIM(EMPL_STATUS.XLATSHORTNAME) AS EMPLOYEE_STATUS_NAME,
RTRIM(EMPL_STATUS.XLATLONGNAME) AS EMPLOYEE_STATUS_DESC,
RTRIM(JOB.EMPL_TYPE) AS EMPLOYEE_TYPE_CODE,
RTRIM(EMPL_TYPE.XLATSHORTNAME) AS EMPLOYEE_TYPE_NAME,
RTRIM(EMPL_TYPE.XLATLONGNAME) AS EMPLOYEE_TYPE_DESC,
RTRIM(JOB.FULL_PART_TIME) AS FULL_PART_TIME_CODE,
RTRIM(FULL_PART_TIME.XLATSHORTNAME) AS FULL_PART_TIME_NAME,
RTRIM(FULL_PART_TIME.XLATLONGNAME) AS FULL_PART_TIME_DESC,
RTRIM(JOB.HR_STATUS) AS HR_STATUS_CODE,
RTRIM(HR_STATUS.XLATSHORTNAME) AS HR_STATUS_NAME,
RTRIM(HR_STATUS.XLATLONGNAME) AS HR_STATUS_DESC,
RTRIM(JOB.JOB_INDICATOR) AS JOB_INDICATOR_CODE,
RTRIM(JOB_INDICATOR.XLATSHORTNAME) AS JOB_INDICATOR_NAME,
RTRIM(JOB_INDICATOR.XLATLONGNAME) AS JOB_INDICATOR_DESC,
RTRIM(JOB.PAY_SYSTEM_FLG) AS PAY_SYSTEM_CODE,
RTRIM(PAY_SYSTEM_FLG.XLATSHORTNAME) AS PAY_SYSTEM_NAME,
RTRIM(PAY_SYSTEM_FLG.XLATLONGNAME) AS PAY_SYSTEM_DESC,
RTRIM(JOB.PER_ORG) AS PERSON_ORG_TYPE_CODE,
RTRIM(PER_ORG.XLATSHORTNAME) AS PERSON_ORG_TYPE_NAME,
RTRIM(PER_ORG.XLATLONGNAME) AS PERSON_ORG_TYPE_DESC,
RTRIM(JOB.POI_TYPE) AS PERSON_OF_INTEREST_CODE,
RTRIM(POI.DESCRSHORT) AS PERSON_OF_INTEREST_NAME,
RTRIM(POI.DESCR) AS PERSON_OF_INTEREST_DESC,
RTRIM(JOB.REG_TEMP) AS REGULAR_TEMPORARY_CODE,
RTRIM(REG_TEMP.XLATSHORTNAME) AS REGULAR_TEMPORARY_NAME,
RTRIM(REG_TEMP.XLATLONGNAME) AS REGULAR_TEMPORARY_DESC,
RTRIM(JOB.SHIFT) AS JOB_SHIFT_CODE,
RTRIM(SHIFT.XLATSHORTNAME) AS JOB_SHIFT_NAME,
RTRIM(SHIFT.XLATLONGNAME) AS JOB_SHIFT_DESC,
RTRIM(JOB.ADDS_TO_FTE_ACTUAL) AS ADDS_TO_FTE_ACTUAL_IND,
RTRIM(JOB.GL_PAY_TYPE) AS GL_PAY_TYPE_CODE,
RTRIM(GL_PAY_TYPE.XLATSHORTNAME) AS GL_PAY_TYPE_NAME, 
RTRIM(GL_PAY_TYPE.XLATLONGNAME) AS GL_PAY_TYPE_DESC,
RTRIM(JOB.ENCUMB_OVERRIDE) AS ENCUMBRANCE_OVERRIDE_IND,
JOB.FTE AS FULL_TIME_EQUIVALENT_QTY,
JOB.ANNUAL_RT AS ANNUAL_RT,
JOB.ANNL_BENEF_BASE_RT AS ANNUAL_BENEFIT_BASE_RT,
JOB.CHANGE_AMT AS CHANGE_AMT,
JOB.CHANGE_PCT AS CHANGE_PCT,
JOB.COMPRATE AS COMPENSATION_RT,
JOB.HOURLY_RT,
JOB.DAILY_RT,
JOB.MONTHLY_RT,
JOB.SHIFT_RT,
JOB.SHIFT_FACTOR AS SHIFT_FACTOR_QTY,
JOB.WORK_DAY_HOURS AS WORK_DAY_HOURS_QTY,
JOB.STD_HOURS AS STANDARD_WEEK_HOURS_QTY
FROM PS_JOB JOB
LEFT OUTER JOIN PS_ACCT_CD_TBL ACCOUNT
ON JOB.ACCT_CD=ACCOUNT.ACCT_CD
LEFT OUTER JOIN PS_ACCT_CD_TBL_GL ACCOUNT_GL
ON ACCOUNT.FDM_HASH=ACCOUNT_GL.FDM_HASH
LEFT OUTER JOIN PS_SET_CNTRL_REC JOB_REC
ON JOB.BUSINESS_UNIT = JOB_REC.SETCNTRLVALUE AND
JOB_REC.RECNAME = &apos;JOB&apos;
LEFT OUTER JOIN PS_HR_LABOR_AGRMNT LABOR_AGREEMENT
ON JOB.LABOR_AGREEMENT=LABOR_AGREEMENT.LABOR_AGREEMENT AND
JOB_REC.SETID = LABOR_AGREEMENT.SETID AND
LABOR_AGREEMENT.EFFDT = (SELECT MAX(X.EFFDT) FROM PS_HR_LABOR_AGRMNT X WHERE LABOR_AGREEMENT.LABOR_AGREEMENT=X.LABOR_AGREEMENT)
LEFT OUTER JOIN PS_POI_TYPE_TBL POI
ON JOB.POI_TYPE=POI.POI_TYPE
LEFT OUTER JOIN PSXLATITEM EMPL_CLASS
ON EMPL_CLASS.FIELDNAME = &apos;EMPL_CLASS&apos;
AND JOB.EMPL_CLASS = EMPL_CLASS.FIELDVALUE
AND EMPL_CLASS.EFFDT = (SELECT MAX(A1.EFFDT) 
	FROM PSXLATITEM A1
	WHERE EMPL_CLASS.FIELDNAME = A1.FIELDNAME 
	AND EMPL_CLASS.FIELDVALUE = A1.FIELDVALUE
	AND EMPL_CLASS.FIELDNAME = &apos;EMPL_CLASS&apos;)
LEFT OUTER JOIN PSXLATITEM BEN_STATUS
ON BEN_STATUS.FIELDNAME = &apos;BEN_STATUS&apos;
AND JOB.BEN_STATUS = BEN_STATUS.FIELDVALUE
AND BEN_STATUS.EFFDT = (SELECT MAX(A1.EFFDT) 
	FROM PSXLATITEM A1
	WHERE BEN_STATUS.FIELDNAME = A1.FIELDNAME 
