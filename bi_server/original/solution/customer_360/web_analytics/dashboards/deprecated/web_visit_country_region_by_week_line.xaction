<?xml version="1.0" encoding="UTF-8"?>
<action-sequence> 
  <title>Web Visits by Country and Region Grouped by Week</title>  
  <version>1</version>  
  <logging-level>DEBUG</logging-level>  
  <documentation> 
    <author>Breadboard BI</author>  
    <description>Returns metrics for country and region, grouped by week.</description>  
    <help/>  
    <icon>file.png|dashboard.png</icon> 
  </documentation>  
  <inputs> 
    <COUNTRY type="string"> 
      <sources> 
        <request>COUNTRY</request> 
      </sources> 
    </COUNTRY>  
    <REGION type="string">
      <sources>
        <request>REGION</request>
      </sources>
    </REGION>
    <WEEK_OF_YEAR type="string">
      <sources>
        <request>WEEK_OF_YEAR</request>
      </sources>
    </WEEK_OF_YEAR>
  </inputs>  
  <outputs> 
    <swresult type="result-set"/> 
  </outputs>  
  <resources/>  
  <actions> 
    <actions> 
      <action-definition> 
        <component-name>SQLLookupRule</component-name>  
        <action-type>Web Visit SQL Query with Country and Region Filters</action-type>  
        <action-inputs> 
          <COUNTRY type="string"/>
          <REGION type="string"/>
          <WEEK_OF_YEAR type="string"/>
        </action-inputs>  
        <action-outputs> 
          <query-result type="result-set" mapping="swresult"/> 
        </action-outputs>  
        <component-definition> 
          <query><![CDATA[SELECT MAX(D.DAY_NAME) AS Day, 
SUM(F.WEB_VISIT_QTY) AS "Unique Visits",
SUM(F.WEB_PAGE_VIEW_QTY) AS "Page Views"
FROM FACT_WEB_SITE_VISIT F, DIMENSION_DAY D, DIMENSION_GEOGRAPHIC_LOCATION G
WHERE F.VISIT_DATE_SK = D.DATE_SK
AND F.GEO_LOCATION_SK = G.GEO_LOCATION_SK
AND G.ISO_3166_COUNTRY_CODE = '{COUNTRY}'
AND G.REGION_NAME = '{REGION}'
AND D.WEEK_SK = '{WEEK_OF_YEAR}'
GROUP BY D.DATE_SK
ORDER BY D.DATE_SK ASC]]></query>
          <jndi>mdw</jndi>
        <live>true</live></component-definition> 
      </action-definition> 
    </actions> 
  </actions> 
</action-sequence>