    <name>DETECT_ROBOTS</name>
    <type>ScriptValue</type>
    <description>This Java Script step detects robot&#47;crawler&#47;spider visitors.</description>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
