<?xml version="1.0" encoding="UTF-8"?>

<report name="wo_top_employees_report" orientation="portrait" pageformat="LETTER" leftmargin="5pt" rightmargin="0pt" topmargin="0" bottommargin="0">
<reportheader>  
<!--
<label fontname="SansSerif" fontsize="14" fontstyle="bold" height="20" vertical-alignment="middle" alignment="left" width="400" x="0" y="0">Top Work Orders by Quantity</label>
-->
</reportheader>
  <groups>
    <group name="dummy">
      <groupheader repeat="true" color="#000000" fontname="SansSerif" fontsize="11" fontstyle="bold" height="14">
        <band name="DummyGroupNoDataBandElement">
          <label height="10" vertical-alignment="middle" alignment="left" width="400" x="0" y="0">No Data Available</label>
        </band>
        <band name="DummyGroupDataBandElement">
          <rectangle color="#808080" draw="false" fill="true" height="12" width="440" x="0" y="0"/>
<!--
          <label color="#D0D0D0" fontname="SansSerif" fontsize="12" fontstyle="bold" height="12" vertical-alignment="middle" alignment="left" width="50" x="0" y="0">Rank</label>
-->
          <label color="#D0D0D0" fontname="SansSerif" fontsize="12" fontstyle="bold" height="12" vertical-alignment="middle" alignment="left" width="150" x="0" y="0">Employee</label>
          <label color="#D0D0D0" fontname="SansSerif" fontsize="12" fontstyle="bold" height="12" vertical-alignment="middle" alignment="left" width="80" x="150" y="0">Quantity</label>
          <label color="#D0D0D0" fontname="SansSerif" fontsize="12" fontstyle="bold" height="12" vertical-alignment="middle" alignment="left" width="80" x="230" y="0">Avg Minutes To Start</label>
          <label color="#D0D0D0" fontname="SansSerif" fontsize="12" fontstyle="bold" height="12" vertical-alignment="middle" alignment="left" width="80" x="310" y="0">Avg Minutes To Complete</label>
        </band>
      </groupheader>
      <groupfooter name="dummyGroupFooter" color="#000000" fontname="SansSerif" fontsize="11" fontstyle="bold">
      </groupfooter>
    </group>
  </groups>
  <items color="#000000" fontname="SansSerif" fontsize="9" fontstyle="bold">
<!--
    <string-field name="RANKElement" fieldname="RANK" vertical-alignment="middle" alignment="left" width="50" x="0" y="0" height="11"/>
-->
    <string-field name="URL-Field" fieldname="FULL_NAME" vertical-alignment="middle" alignment="left" width="150" x="0" y="0" height="11"/>
    <number-field name="WORK_ORDER_QTYElement" fieldname="WORK_ORDER_QTY" vertical-alignment="middle" alignment="left" width="80" x="150" y="0" height="11"/>
    <number-field name="START_MINUTE_QTYElement" fieldname="START_MINUTE_QTY" format="#,##0" vertical-alignment="middle" alignment="left" width="80" x="230" y="0" height="11"/>
        <number-field name="COMPLETE_MINUTE_QTYElement" fieldname="COMPLETE_MINUTE_QTY" format="#,##0" vertical-alignment="middle" alignment="left" width="80" x="310" y="0" height="11"/>
  </items>
  <functions>
    <expression class="org.jfree.report.function.HideElementIfDataAvailableExpression" name="DummyGroupNoDataBandElementHideIfNullTrigger">
      <properties>
        <property name="element">DummyGroupNoDataBandElement</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="DummyGroupDataBandElementHideIfNullTrigger">
      <properties>
        <property name="element">DummyGroupDataBandElement</property>
      </properties>
    </expression>
    <expression class="org.jfree.report.function.ShowElementIfDataAvailableExpression" name="dummyGroupFooterHideIfNullTrigger">
      <properties>
        <property name="element">dummyGroupFooter</property>
      </properties>
    </expression>
 
