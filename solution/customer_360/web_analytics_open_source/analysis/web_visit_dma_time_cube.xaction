<?xml version="1.0" encoding="UTF-8"?>
<action-sequence> 
  <name>web_visit_dma_time_cube.xaction</name>  
  <title>%title</title>  
  <version>2</version>  
  <logging-level/>  
  <documentation> 
    <author>Breadboard BI, LLC</author>  
    <help>1. Make sure the path to your breadboardbi.mondrian.xml file is correct. 2. Do not forget to create a JNDI connection named "mdw". 3. Watch for case sensitivity issues with some databases. 4. Change the year value if you do not have data for 2006.</help>  
    <result-type>report</result-type>  
    <description>%description</description>  
    <icon>file.png|analysis.png</icon> 
  </documentation>  
  <inputs> 
    <mdx type="string"> 
      <default-value/>  
      <sources> 
        <request>MDX</request> 
      </sources> 
    </mdx>  
    <mode type="string"> 
      <sources> 
        <request>mode</request> 
      </sources>  
      <default-value/> 
    </mode>  
    <dma type="string"> 
      <sources> 
        <request>dma</request> 
      </sources>  
      <default-value/> 
    </dma> 
  </inputs>  
  <outputs> 
    <model type="string"/>  
    <connection type="string"/>  
    <mdx type="string"/>  
    <options type="list"/>  
    <title type="string"/>  
    <url type="string"> 
      <destinations> 
        <response>redirect</response> 
      </destinations> 
    </url> 
  </outputs>  
  <resources/>  
  <actions> 
    <action-definition> 
      <component-name>PivotViewComponent</component-name>  
      <action-inputs> 
        <mode type="string"/>  
        <period type="string"/>  
        <dma type="string"/> 
      </action-inputs>  
      <action-outputs> 
        <model type="string"/>  
        <connection type="string"/>  
        <mdx type="string"/>  
        <options type="list"/>  
        <title type="string"/>  
        <url type="string"/> 
      </action-outputs>  
      <component-definition> 
        <title>%title</title>   
        <viewer>Pivot</viewer>  
        <model>breadboard/analysis/breadboardbi.mondrian.xml</model>  
        <location>mondrian</location>  
        <options> 
          <personal/>  
          <cube-nav/>  
          <mdx-edit/>  
          <sort-conf/>  
          <spacer/>  
          <level-style/>  
          <hide-spans/>  
          <properties/>  
          <non-empty/>  
          <swap-axes/>  
          <spacer/>  
          <drill-member/>  
          <drill-position/>  
          <drill-replace/>  
          <drill-thru/>  
          <spacer/>  
          <chart/>  
          <chart-conf/>  
          <spacer/>  
          <print-conf/>  
          <print-pdf/>  
          <spacer/>  
          <excel/> 
        </options>  
        <jndi>mdw_mysql</jndi>  
        <query><![CDATA[select NON EMPTY {[Measures].[Visits], [Measures].[Previous Period Visit Variance], 
[Measures].[Previous Period % Visit Change], [Measures].[Ratio To Parent], 
[Measures].[QTD Visits], [Measures].[MTD Visits]} ON COLUMNS,
NON EMPTY {([Time Period].[All Periods], 
[Web Visitor Location.Web Visitor DMAs].[All DMAs].[United States]{dma}), 
([Time Period].[All Periods].[2006],
[Web Visitor Location.Web Visitor DMAs].[All DMAs].[United States]{dma}),
([Time Period].[All Periods].[2007],
[Web Visitor Location.Web Visitor DMAs].[All DMAs].[United States]{dma})} ON ROWS
from [Web Site Visits]]]></query> 
      </component-definition>  
      <action-name>Pivot View</action-name>  
      <logging-level>debug</logging-level>  
      <action-type/> 
    </action-definition> 
  </actions> 
</action-sequence>