           <field_name/>
           <schema_name/>
           </partitioning>
      <schema/>
      <table>DIMENSION_WEB_FILE</table>
      <connection>mdw_mysql</connection>
      <commit>0</commit>
      <update>Y</update>
      <fields>
        <key>
          <name>NEW_WEB_FILE_NAME</name>
          <lookup>WEB_FILE_NAME</lookup>
        </key>
        <date>
          <name/>
          <from>DW_START_DATE</from>
          <to>DW_STOP_DATE</to>
